<html>
  {{> head}}
  <body class="dashboard">
    <div class="d-flex" id="wrapper">
      {{> sidebar}}
      <div id="page-content-wrapper">
        {{> navbar}}
        <div class="container-fluid">
          <input type="hidden" id="type" name="type" value="{{type}}" />
            <div class="row mt-4">
              <div class="col-sm-6">
                <div class="breadcrumb-custom">
                  <ul>
                    <li>
                      <a href="/">Home</a>
                    </li>
                    <li>
                      <a href="/member.html">Member</a>
                    </li>
                    <li class="active">
                      Detail
                    </li>
                  </ul>
                </div>
              </div>
              <div class="col-sm-6 text-right">
                <form id="form_cetak">
                  <select id="bulan_cetak" name="bulan_cetak" class="form-control w-auto d-inline-block">
                    <option value="1">Januari</option>
                    <option value="2">Februari</option>
                    <option value="3">Maret</option>
                    <option value="4">April</option>
                    <option value="5">Mei</option>
                    <option value="6">Juni</option>
                    <option value="7">Juli</option>
                    <option value="8">Agustus</option>
                    <option value="9">September</option>
                    <option value="10">Oktober</option>
                    <option value="11">November</option>
                    <option value="12">Desember</option>
                  </select>
                  <input type="text" id="tahun_cetak" name="tahun_cetak" required class="form-control w-auto d-inline-block" placeholder="Tahun">
                  <input type="hidden" id="id_cetak" value="{{id}}" />
                  <button id="btn_cetak" type="submit" class="btn btn-primary btn-cetak w-auto d-inline-block align-top"><span class="fa fa-file-pdf-o"></span> Cetak</button>
                </form>
              </div>
            </div>
            <br>
            <input type="hidden" id="id" name="id" value="{{id}}" />
            <input type="hidden" id="member_id" name="member_id" value="" />
            <input type="hidden" id="server_id_unselected" name="server_id_unselected" value="{{server_id_unselected}}" />
            <div class="row">
              <div class="col-md-10">
                <div class="card stat-card">
                  <div class="card-body">
                    <div class="row">
                      <div class="col-md-5" id="ppp_div">
                        <h6 class="ml-1"><b>PPP</b></h6>
                        <div class="table-responsive">
                          <table class="table table-sm table-borderless">
                            <tr>
                              <td>Name</td>
                              <td> : </td>
                              <td id="name"></td>
                            </tr>
                            <tr>
                              <td>Password</td>
                              <td> : </td>
                              <td id="password"></td>
                            </tr>
                            <tr>
                              <td>Profile</td>
                              <td> : </td>
                              <td id="profile"></td>
                            </tr>
                            <tr>
                              <td>Remote Address</td>
                              <td> : </td>
                              <td id="remote_address"></td>
                            </tr>
                          </table>
                        </div>
                      </div>
                      <div class="col-md-5" id="sq_div">
                        <h6 class="ml-1"><b>PPP</b></h6>
                        <div class="table-responsive">
                          <table class="table table-sm table-borderless">
                            <tr>
                              <td>Name</td>
                              <td> : </td>
                              <td id="name_sq"></td>
                            </tr>
                            <tr>
                              <td>Target</td>
                              <td> : </td>
                              <td id="target_sq"></td>
                            </tr>
                            <tr>
                              <td>Upload Max Limit</td>
                              <td> : </td>
                              <td id="upload_max_limit_sq"></td>
                            </tr>
                            <tr>
                              <td>Download Max Limit</td>
                              <td> : </td>
                              <td id="download_max_limit_sq"></td>
                            </tr>
                          </table>
                        </div>
                      </div>
                      <div class="col-md-7">
                        <h6 class="ml-1"><b>Member</b></h6>
                        <div class="row">
                          <div class="col-md-12">
                            <table class="table table-sm table-borderless">
                              <tr>
                                <td>Nama</td>
                                <td> : </td>
                                <td id="nama"></td>
                              </tr>
                              <tr>
                                <td>Alamat</td>
                                <td> : </td>
                                <td id="alamat"></td>
                              </tr>
                              <tr>
                                <td>No Whatsapp</td>
                                <td> : </td>
                                <td id="no_wa"></td>
                              </tr>
                              <tr>
                                <td>Email</td>
                                <td> : </td>
                                <td id="email"></td>
                              </tr>
                              <tr>
                                <td>Tagihan Dimulai Pada</td>
                                <td> : </td>
                                <td id="awal_tagihan"></td>
                              </tr>
                              <tr id="username_sq_div">
                                <td>Username</td>
                                <td> : </td>
                                <td id="username_sq"></td>
                              </tr>
                              <tr id="password_sq_div">
                                <td>Password</td>
                                <td> : </td>
                                <td id="password_sq"></td>
                              </tr>
                            </table>
                          </div>
                        </div>

                      </div>
                    </div>
                  </div>
                </div>
                <br>
              </div>
              <div class="col-md-2">
                <div class="widget widget-lg widget-danger w-100">
                  <div class="widget-wrapper">
                    <div class="widget-style">&nbsp;</div>
                    <div class="widget-content">
                      <div class="text">Nominal Pembayaran</div>
                      <div class="count" id="nominal_pembayaran">0</div>
                    </div>
                  </div>
                </div>
                <br>
                <br>
                <div class="widget widget-lg widget-danger w-100" id="widget_is_active" style="display:none;">
                  <div class="widget-wrapper">
                    <div class="widget-style">&nbsp;</div>
                    <div class="widget-content">
                      <div class="count" id="is_active">ONLINE</div>
                    </div>
                  </div>
                </div>
              </div>
              <div class="col-md-12">
                <ul class="nav nav-tabs">
                  <li class="nav-item" role="presentation" id="li_traffic">
                    <a class="nav-link active" data-toggle="tab" href="#traffic">Traffic</a>
                  </li>
                  <li class="nav-item" role="presentation">
                    <a id="tab_inventaris_alat" class="nav-link" data-toggle="tab" href="#inventaris_alat">Inventaris Alat</a>
                  </li>
                </ul>
                <div class="tab-content">
                  <div class="tab-pane fade show active" id="traffic">
                    <div class="stat-card">
                      <div class="">
                        <div class="text-right">
                          <form class="form_tgl">
                            <div class="form-group d-inline-block">
                              <div class="input-group input-group-sm">
                                <div class="input-group-prepend"><span class="input-group-text bg-primary text-white"><i class="fa fa-calendar"></i></span></div>
                                <input type="text" name="tgl" id="tgl" class="daterange form-control" placeholder="Tgl" />
                                <div class="input-group-append "><button class="btn btn-danger clear" type="button">Clear</button></div>
                              </div>
                            </div>
                          </form>
                        </div>
                        <div id="data">
                          <canvas id="chart" height="100"></canvas>
                        </div>
                      </div>
                    </div>
                  </div>
                  <div class="tab-pane fade" id="inventaris_alat">
                    <div class="text-right">
                      <a href="javascript:void(0);" onclick="modal_inventaris_alat(1,this)" class="btn btn-primary"><span class="fa fa-plus"></span> Tambah</a>
                    </div>
                    <br>
                    <div>
                      <table class="table table-striped">
                        <thead>
                          <th>No</th>
                          <th>Nama Alat</th>
                          <th>Serial Number</th>
                          <th>Merek</th>
                          <th>Tgl Pasang</th>
                          <th>Aksi</th>
                        </thead>
                        <tbody id="listdata">

                        </tbody>
                      </table>
                    </div>
                    <div class="row">
                      <div class="col-md-4" id="info_page">

                      </div>
                      <div class="col-md-4 text-center" id="pagination">
                      </div>
                      <div class="col-md-4"></div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <br>
          <br><br>
        </div>
      </div>
      <!-- /#page-content-wrapper -->
    </div>
    <div class="modal" id="modal_inventaris_alat">
      <div class="modal-dialog">
        <div class="modal-content">
          <div class="modal-header">
            <h5 class="modal-title" id="modal_inventaris_alat_title"></h5>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
              <span aria-hidden="true">&times;</span>
            </button>
          </div>
          <div class="modal-body">
            <form id="form_inventaris_alat">
              <input type="hidden" id="inventaris_alat_id" name="inventaris_alat_id" />
              <div class="form-group">
                <label class="control-label">Nama</label>
                <input id="nama_alat" name="nama_alat" type="text" class="form-control" placeholder="Nama Alat" required />
              </div>
              <div class="form-group">
                <label class="control-label">Serial Number</label>
                <input id="serial_number" name="serial_number" type="text" class="form-control" placeholder="Serial Number" required />
              </div>
              <div class="form-group">
                <label class="control-label">Merek</label>
                <input id="merek" name="merek" type="text" class="form-control" placeholder="Merek" required />
              </div>
              <div class="form-group">
                <label class="control-label">Tgl Pasang</label>
                <input type="text" id="tgl_pasang" name="tgl_pasang" class="form-control" placeholder="Tgl Pasang" required />
              </div>
              <div class="text-right">
                <button type="button" class="btn btn-light" data-dismiss="modal">Batal</button>
                <button type="submit" class="btn btn-primary">Simpan</button>
              </div>
            </form>
          </div>
        </div>
      </div>
    </div>
    <!-- /#wrapper -->
    {{> foot}}
    <script type="text/javascript" src="/assets/js/page/member_detail.js?v=1.1"></script>
  </body>
</html>
